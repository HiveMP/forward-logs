{
  "name": "forward-logs",
  "version": "1.0.0",
  "description": "Forwards the stdout and stderr of a command to a websocket server, as defined by environment variables.",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^10.12.2",
    "@types/which": "^1.3.1",
    "gulp": "^4.0.0",
    "gulp-cli": "^2.0.1",
    "mkdirp": "^0.5.1",
    "prettier": "^1.14.3",
    "ts-node": "^7.0.1",
    "typescript": "^3.1.6"
  },
  "dependencies": {
    "nexe": "^3.0.0-beta.7",
    "node-pty": "^0.7.8",
    "websocket-stream": "^5.1.2",
    "which": "^1.3.1"
  },
  "repository": {
    "url": "https://github.com/HiveMP/forward-logs"
  },
  "scripts": {
    "start": "ts-node index.ts",
    "start-native": "tsc && nexe index.js && forward-logs",
    "build": "tsc && nexe index.js",
    "build-echo": "nexe echo.js",
    "build-release": "tsc && gulp && nexe --input index.js -o dist/forward-logs"
  }
}
